  "authors": [
    "fzzyhmstrs"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/timefall_coin",
    "issues": "https://github.com/fzzyhmstrs/timefall_coin/issues",
    "sources": "https://github.com/fzzyhmstrs/timefall_coin"
  },
  "icon": "assets/timefall_coin/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "me.fzzyhmstrs.timefall_coin.TC"
      }
    ]
  },
  "depends": {
    "fabricloader": ">=0.14.22",
    "fabric": "*",
    "fabric-key-binding-api-v1": "*",
    "fabric-language-kotlin": ">=1.7.4+kotlin.1.6.21",
    "minecraft": ">=1.20.1",
